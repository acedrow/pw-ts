{
  "compilerOptions": {
    "moduleResolution": "node",
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "lib": ["es6", "dom"],

    "esModuleInterop": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "allowJs": true,
    "sourceMap": true,
    "noImplicitThis": true,

  }
}